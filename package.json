{
  "name": "cofhejs",
  "version": "0.0.1",
  "description": "Implements js  library for CoFHE",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "dev": "vitest",
    "format": "prettier --write --ignore-path=.gitignore --ignore-path=.prettierignore .",
    "check-format": "prettier --check --ignore-path=.gitignore --ignore-path=.prettierignore .",
    "check-exports": "attw --pack . --ignore-rules=cjs-resolves-to-esm",
    "ci": "npm run build && npm run check-format && npm run check-exports"
  },
  "author": "",
  "license": "BSD-3-Clause",
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.17.4",
    "@changesets/cli": "^2.29.3",
    "prettier": "^3.5.3",
    "typescript": "^5.8.3",
    "vitest": "^3.1.2"
  },
  "dependencies": {
    "bignumber.js": "^9.3.0"
  },
  "keywords": [
    "cofhe",
    "javascript",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/openvectorai/cofhejs.git"
  },
  "files": [
    "dist"
  ],
  "type": "module"
}
